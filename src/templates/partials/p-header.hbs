                                <label for="villeFilter">Ville</label>
                            </div>
                            <div class="form-group form-checkbox">
                                <input id="societeFilter" class="form-fild" type="checkbox" name="showSociete" value="societe">
                                <label for="societeFilter">Société</label>
                            </div>
                            <div class="form-group form-checkbox">
                                <input id="emploiFilter" class="form-fild" type="checkbox" name="showEmploi" value="emploi">
                                <label for="emploiFilter">Emploi</label>
                            </div>
                        </div>
